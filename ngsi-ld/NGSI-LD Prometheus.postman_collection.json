					"event": [
						{
							"listen": "test",
							"script": {
								"id": "3fed1773-8774-4326-806a-4d01db8e4040",
								"exec": [
									"pm.collectionVariables.set(\"csource1\", pm.response.headers.get(\"Location\"));"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json",
