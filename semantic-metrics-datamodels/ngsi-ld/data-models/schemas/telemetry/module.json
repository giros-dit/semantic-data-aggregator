{
    "$schema": "http://json-schema.org/schema#",
    "$schemaVersion": "1.0",
    "title": "Module",
    "description": "Entity that represents an implementation of a YANG module",
    "type": "object",
    "additionalProperties": false,
    "allOf": [
        {
            "$ref": "../entity.json#/definitions/Entity"
        },
        {
            "properties": {
                "type": {
                    "description": "NGSI-LD Entity identifier. It has to be Module",
                    "type": "string",
                    "enum": [
                        "Module"
                    ],
                    "default": "Module"
                },
                "implementedBy": {
                    "oneOf": [
                        {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/ImplementedBy"
                            }
                        },
                        {
                            "$ref": "#/definitions/ImplementedBy"
                        }
                    ]
                },
                "name": {
                    "$ref": "#/definitions/Name"
                },
                "namespace": {
                    "$ref": "#/definitions/Namespace"
                },
                "revision": {
                    "$ref": "#/definitions/Revision"
                }
            },
            "required": [
                "type",
                "implementedBy",
                "name",
                "namespace",
                "revision"
            ]
        }
    ],
    "definitions": {
        "Deviation": {
            "description": "Property. Name of YANG module that deviates this YANG module.",
            "type": "object",
            "allOf": [
                {
                    "$ref": "../entity.json#/definitions/Property"
                },
                {
                    "properties": {
                        "value": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "value"
                    ]
                }
            ],
            "additionalProperties": false
        },
        "ImplementedBy": {
            "description": "Relationship. Network device that implements this YANG module.",
            "type": "object",
            "allOf": [
                {
                    "$ref": "../entity.json#/definitions/Relationship"
                },
                {
                    "properties": {
                        "object": {
                            "type": "string"
                        },
                        "deviation": {
                            "$ref": "#/definitions/Deviation"
                        },
                        "feature": {
                            "$ref": "#/definitions/Feature"
                        }
                    },
                    "required": [
                        "object"
                    ]
                }
            ],
            "additionalProperties": false
        },
        "Feature": {
            "description": "Property. Name of feature that the target network device implements for this YANG module.",
            "type": "object",
            "allOf": [
                {
                    "$ref": "../entity.json#/definitions/Property"
                },
                {
                    "properties": {
                        "value": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "value"
                    ]
                }
            ],
            "additionalProperties": false
        },
        "Name": {
            "description": "Property. Name of the YANG module.",
            "type": "object",
            "allOf": [
                {
                    "$ref": "../entity.json#/definitions/Property"
                },
                {
                    "properties": {
                        "value": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "value"
                    ]
                }
            ],
            "additionalProperties": false
        },
        "Namespace": {
            "description": "Property. Namespace of the YANG module.",
            "type": "object",
            "allOf": [
                {
                    "$ref": "../entity.json#/definitions/Property"
                },
                {
                    "properties": {
                        "value": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "value"
                    ]
                }
            ],
            "additionalProperties": false
        },
        "Revision": {
            "description": "Property. Revision date of the YANG module.",
            "type": "object",
            "allOf": [
                {
                    "$ref": "../entity.json#/definitions/Property"
                },
                {
                    "properties": {
                        "value": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "value"
                    ]
                }
            ],
            "additionalProperties": false
        },
    }
}
